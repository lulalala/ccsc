<p id="notice"><%= notice %></p>

<h1>Listing Fountains</h1>

<table class="table">
  <tbody>
    <% @fountains.each do |fountain| %>
      <tr>
        <td><%= fountain.issue %></td>
        <td><%= link_to fountain.title, fountain %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @fountains %>
